{
  "name": "@softmedialab/materialui-table",
  "version": "1.0.50",
  "description": "Datatable for React based on https://material-table.com with additional features",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "babel": {
    "extends": "./configs/.babelrc"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./demo/webpack.config.js --mode development",
    "build": "babel src --out-dir dist",
    "lint": "npm run eslint && npm run tsc",
    "eslint": "eslint src/** -c ./configs/.eslintrc",
    "tsc": "tsc --noEmit --lib es6,dom --skipLibCheck types/index.d.ts",
    "lint:fix": "eslint src/** --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbatapCompany/materialui-table.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "material",
    "datatable",
    "table"
  ],
  "author": "Mehmet Baran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AbatapCompany/materialui-table/issues"
  },
  "homepage": "https://github.com/AbatapCompany/materialui-table#readme",
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.4",
    "babel-polyfill": "6.26.0",
    "eslint": "7.12.1",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-react": "7.27.0",
    "husky": "7.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.4.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.11"
  },
  "dependencies": {
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@material-ui/pickers": "3.3.10",
    "@mui/icons-material": "5.1.0",
    "@mui/lab": "5.0.0-alpha.54",
    "@mui/material": "5.1.0",
    "@mui/styles": "5.1.0",
    "@mui/system": "5.1.0",
    "ahooks": "2.10.12",
    "classnames": "2.3.1",
    "date-fns": "2.25.0",
    "debounce": "1.2.1",
    "filefy": "0.1.11",
    "prop-types": "15.7.2",
    "react-beautiful-dnd": "13.1.0",
    "react-double-scrollbar": "0.0.15"
  }
}
